1 VERSIONINFO

FILEVERSION ${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH}
PRODUCTVERSION ${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH}
FILEFLAGSMASK 0x17L

#ifdef _DEBUG
  FILEFLAGS 0x1L
#else
  FILEFLAGS 0x0L
#endif
  FILEOS 0x4L
  FILETYPE 0x0L
  FILESUBTYPE 0x0L
  
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "FileDescription","${CURRENT_TARGET}"
      VALUE "FileVersion","${PROJECT_VERSION}"
      VALUE "InternalName","${CURRENT_TARGET}"
      VALUE "CompanyName","You Guess"
      VALUE "LegalCopyright","Copyright (C) ${CURRENT_YEAR}"
      VALUE "OriginalFilename"," "
      VALUE "ProductName","${CMAKE_PROJECT_DESCRIPTION}"
      VALUE "ProductVersion","${CMAKE_PROJECT_VERSION}"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation",0x00,0
  END
END
